@model IList<PhimHang.Controllers.UserRandom>
<div class="second-left-banner">
    <div class="banner-header-white">DÂN PHÍM CHUYÊN NGHIỆP</div>
    @{
        foreach (var item in Model)
        {
            <div class="banner-item-user-detail">
                <div class="avata-genaral-size">
                    <img src="@item.Avata?width=50&height=53&mode=crop" class="img-rounded" alt="avatar">
                </div>
                <div class="list-item-detail">
                    <div class="list-item-username danphim">
                        <a href="/@item.UserName">@item.FullName<i class="fa fa-check-circle"></i></a><br />
                        <a class="borkerVipStyle" href="/@item.UserName">&#64;@item.UserName</a>
                    </div>
                    @*<div class="list-item-control">
                        <a href="/@item.UserName" class="button"><i class="fa fa-user"></i> Xem thêm</a>
                    </div>*@
                </div>
            </div>
        }
    }


    @*<div class="loadmore"><a href="#"><i class="fa  fa-angle-double-down"></i></a></div>*@

</div>

<div class="banner-info" style="color: #666;">
    <div >
        <a class="banner-info-userpage" href="/gioithieu">Giới thiệu</a>
        <a class="banner-info-userpage" href="/quydinh">Quy định</a>
        <a class="banner-info-userpage" href="/huongdansudung">Trợ giúp</a>
        <a class="banner-info-userpage" href="/lienhe">Liên hệ</a>
    </div>
    <p>
        Bản quyền thuộc về Phố chứng khoán<br>
        Copyright © phochungkhoan.com
    </p>
</div>
          