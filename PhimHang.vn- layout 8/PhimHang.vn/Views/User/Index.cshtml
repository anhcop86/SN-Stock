
@{
    ViewBag.Title = @Model.FullName + " home page. https://cungphim.com";
    Layout = "~/Views/Shared/_Layout_User.cshtml";
}
@section scripts{
    <script src="~/Scripts/jquery.timeago.js"></script>    
}
@{
    if(ViewBag.TabId == 1)
    {
        @Html.Partial("_PartialUserTab1")
    }
    else if(ViewBag.TabId == 2)
    {
        @Html.Partial("_PartialUserTab2")
    }
    else if(ViewBag.TabId == 3)
    {
        @Html.Partial("_PartialUserTab3")
    }
    else
    {
        @Html.Partial("_PartialUserTab4")
    }
    }
