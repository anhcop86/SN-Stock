<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" namespace="ShareHolderCore.Domain.Model" assembly="ShareHolderCore">
	<class name="ShareHolderCore.Domain.Model.TradingTransaction,ShareHolderCore" table="TradingTransaction" lazy="false">

		<!-- Primary Key(s) -->
		<id name="TransactionId" column="TransactionId" type="Int64" unsaved-value="0">
			<generator class="native"/>
		</id>
		
		<!-- Foreign Keys not mapped as Many-To-One relations -->
		<!-- more than one foreign column is mapped to TransactionCategoryId - you're on your own. -->
		<many-to-one name="TransactionCategory"  column="TransactionCategoryId" class="ShareHolderCore.Domain.Model.TransactionCategory,ShareHolderCore" />
		<!-- Properties -->
		<property column="TransactionNumber" type="String" name="TransactionNumber" not-null="true" length="255" />
		<property column="TransactionDate" type="DateTime" name="TransactionDate" />
		<property column="Note" type="String" name="Note" length="255" />
		<property column="CompanyName" type="String" name="CompanyName" length="255" />
		
		<!-- One-To-Many relations -->
		<bag name="ListTransactionDetail" generic="true" inverse="true" cascade="none" lazy="false">
			<key column="TransactionId"/>
			<one-to-many class="ShareHolderCore.Domain.Model.TransactionDetail,ShareHolderCore" />
		</bag>
		
	</class>
</hibernate-mapping>
