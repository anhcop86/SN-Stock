<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" namespace="ShareHolderCore.Domain.Model" assembly="ShareHolderCore">
	<class name="ShareHolderCore.Domain.Model.ShareType,ShareHolderCore" table="ShareType" lazy="true">

		<!-- Primary Key(s) -->
		<id name="ShareTypeId" column="ShareTypeId" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		
		<!-- Properties -->
		<property column="Description" type="String" name="Description" length="255" />
		
		<!-- One-To-Many relations -->
		<bag name="ListTransactionDetail" generic="true" inverse="true" cascade="none" lazy="true">
			<key column="ShareTypeId"/>
			<one-to-many class="ShareHolderCore.Domain.Model.TransactionDetail,ShareHolderCore" />
		</bag>
		
	</class>
</hibernate-mapping>
