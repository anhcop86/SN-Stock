<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" namespace="ShareHolderCore.Domain.Model" assembly="ShareHolderCore">
  <class name="ShareHolderCore.Domain.Model.Province,ShareHolderCore" table="Province" lazy="true">

    <!-- Primary Key(s) -->
    <id name="ProvinceId" column="ProvinceId" type="Int32" unsaved-value="0">
      <generator class="native"/>
    </id>

    <!-- Properties -->
    <!--<property column="CountryId" type="Int16" name="CountryId" />-->    
    <property column="Name" type="String" name="Name" length="70" />
    <property column="EnglishName" type="String" name="EnglishName" length="70" />
    <property column="CreatedDate" type="String" name="CreatedDate" length="14" />
    <property column="CreatedBy" type="String" name="CreatedBy" length="14" />
    

    <!-- One-To-Many relations -->
      <!--<bag name="HotelFacility" generic="true" inverse="true" cascade="none" lazy="true">
        <key column="FacilityId"/>
        <one-to-many class="ShareHolderCore.Domain.Model.HotelFacility,ShareHolderCore" />
      </bag>-->
    
    
    <!-- Many to one relations -->
    <many-to-one name="Country"  column="CountryId" class="ShareHolderCore.Domain.Model.Country,ShareHolderCore" lazy="false" />

    <!-- One-To-Many relations -->
    <bag name="ListHotel" generic="true" inverse="true" cascade="none" lazy="true">
      <key column="ProvinceId"/>
      <one-to-many class="ShareHolderCore.Domain.Model.Hotel,ShareHolderCore" />
    </bag>

  </class>
</hibernate-mapping>

