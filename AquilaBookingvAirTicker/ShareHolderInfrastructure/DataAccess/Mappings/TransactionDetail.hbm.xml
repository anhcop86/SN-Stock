<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" namespace="ShareHolderCore.Domain.Model" assembly="ShareHolderCore">
	<class name="ShareHolderCore.Domain.Model.TransactionDetail,ShareHolderCore" table="TransactionDetail" lazy="true">

		<!-- Primary Key(s) -->
		<id name="TransactionDetailId" column="TransactionDetailId" type="Int64" unsaved-value="0">
			<generator class="native"/>
		</id>
		
		<!-- Foreign Keys not mapped as Many-To-One relations -->
		<property column="TransactionId" type="Int64" name="TransactionId" not-null="true" />
		<property column="ShareSymbol" type="String" name="ShareSymbol" not-null="true" length="50" />		
		<property column="ShareTypeId" type="Int32" name="ShareTypeId" />
		<many-to-one name="ShareHolder"  column="ShareHolderId" class="ShareHolderCore.Domain.Model.ShareHolder,ShareHolderCore" />
		<!-- Properties -->
		<property column="TransactionType" type="String" name="TransactionType" length="6" />
		<property column="Quantity" type="Int64" name="Quantity" />
		
	</class>
</hibernate-mapping>
