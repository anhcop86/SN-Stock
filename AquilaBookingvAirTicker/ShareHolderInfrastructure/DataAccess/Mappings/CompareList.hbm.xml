<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" namespace="ShareHolderCore.Domain.Model" assembly="ShareHolderCore">
  <class name="ShareHolderCore.Domain.Model.CompareList,ShareHolderCore" table="CompareList" lazy="true">

    <!-- Primary Key(s) -->
    <id name="CompareListId" column="CompareListId" type="Int64" unsaved-value="0">
      <generator class="native"/>
    </id>

    <!-- Properties -->
    <!--<property column="MemberId" type="Int32" name="MemberId" />-->
    <property column="CreatedDate" type="String" name="CreatedDate"/>
    <property column="CreatedBy" type="String" name="CreatedBy"/>
    

    <!-- One-To-Many relations -->
    <bag name="ListCompareListDetail" generic="true" inverse="true" cascade="none" lazy="true">
      <key column="CompareListId"/>
      <one-to-many class="ShareHolderCore.Domain.Model.CompareListDetail,ShareHolderCore" />
    </bag>

    <many-to-one name="Membership"  column="MemberId" class="ShareHolderCore.Domain.Model.Membership,ShareHolderCore" />

  </class>
</hibernate-mapping>

