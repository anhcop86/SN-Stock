<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping  xmlns="urn:nhibernate-mapping-2.2" namespace="ShareHolderCore.Domain.Model" assembly="ShareHolderCore" >
	
	
	<sql-query name="spTransactionDetailOfShareHolder">
		<query-param name="TransactionToDate"  type="DateTime"   />
		<query-param name="ShareHolderCode"  type="String"   />
		<query-param name="ShareSymbol"    type="String"  />
		<query-param name="OrderBy"   type="String"  />
		<query-param name="OrderDirection"   type="String"   />
		<query-param name="Page"  type="Int32"    />
		<query-param name="PageSize"  type="Int32"    />
		<query-param name="TotalRecords"  type="Int32"  />
		
			<return-scalar column="TransactionNumber"  type="String" />
			<return-scalar column="TransactionId"  type="Int64" />
			<return-scalar column="TransactionDate"  type="DateTime"  />
			<return-scalar column="Name" type="String"    />
			<return-scalar column="ShareSymbol" type="String"     />
			<return-scalar column="OpenningBalance"   type="Int64"    />
			<return-scalar column="Credit"   type="Int64"  />
			<return-scalar column="Debit"  type="Int64"  />
			<return-scalar column="CurrentBalance"  type="Int64"    />
			<return-scalar column="ShareHolderCode"  type="String"  />
			<return-scalar column="InteractiveShareHolder"  type="String"   />
			<return-scalar column="DescriptionType" type="String"   />
			<return-scalar column="Note"  type="String"  />
		
		exec spTransactionDetailOfShareHolder
		:TransactionToDate,
		:ShareHolderCode,
		:ShareSymbol,
		:OrderBy,
		 :OrderDirection,
		:Page,
		:PageSize,
		:TotalRecords
	</sql-query>
</hibernate-mapping>
